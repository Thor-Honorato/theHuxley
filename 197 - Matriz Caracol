#include <stdio.h>

int main ()
{
	int n, i = 0, j = 0; //Dimens√£o da matriz.
	scanf ("%d", &n);
	int matriz[n][n];
	int esquerda = 0, direita = n, topo = 0, fundo = n, fim = n*n;
	while (fim != 0)
	{
		for ( ; j < fundo && fim != 0; j++)
		{
			scanf ("%d", &matriz[i][j]);
			fim--;
		}
		j--;
		esquerda++;
		for (i = i+1; i < direita && fim != 0; i++)
		{
			scanf ("%d", &matriz[i][j]);
			fim--;
		}
		fundo--;
		i--;
		for (j = j-1; j >= topo && fim != 0; j--)
		{
			scanf ("%d", &matriz[i][j]);
			fim--;
		}
		direita--;
		j++;
		for (i = i-1; i >= esquerda && fim != 0; i--)
		{
			scanf ("%d", &matriz[i][j]);
			fim--;
		}
		i++;
		j++;
		topo++;
	}

	for (j = 0; j < n; j++)
	{
		for (i = 0; i < n; i++)
		{
			printf("%d\n", matriz[i][j]);
		}
	}

	return 0;
}
