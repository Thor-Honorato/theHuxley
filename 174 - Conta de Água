#include <stdio.h>

int main () 
{
	int n, conta;
	scanf ("%d", &n);

	if (n <= 10) {
		printf("7\n");	
	}
	else if (n > 10 && n <= 30) {
		n = n - 10;
		conta = 1 * n;
		conta = conta + 7;
		printf("%d\n", conta);
	}
	else if (n > 30 && n <= 100) {
		int aux = 27;
		n = n - 30;
		conta = 2 * n;
		conta = conta + aux;
		printf("%d\n", conta);
	}
	else {
		int aux = 167;
		n = n - 100;
		conta = 5 * n;
		conta = conta + aux;
		printf("%d\n", conta);
	}

	return 0;
}
